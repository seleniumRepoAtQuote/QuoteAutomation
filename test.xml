		<parameter name="platform" value="Mac OSX 10.14" />
		<parameter name="browserName" value="Chrome" />
		<parameter name="version" value="87" />
		<parameter name="screenResolution" value="1366x768" />
		<classes>
			<class name="org.quote.mdaTests.CarInsuranceComparison" />
		</classes>
	</test>
